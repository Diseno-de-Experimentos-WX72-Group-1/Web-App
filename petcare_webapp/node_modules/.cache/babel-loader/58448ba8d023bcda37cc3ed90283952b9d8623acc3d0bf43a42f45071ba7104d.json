{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'RegistroComponent',\n  data() {\n    return {\n      nombre: '',\n      correo: '',\n      contraseña: '',\n      rol: 'dueno',\n      mensaje: ''\n    };\n  },\n  methods: {\n    async registrarUsuario() {\n      try {\n        const response = await axios.post('http://localhost:8080/api/usuarios/registrar', {\n          nombre: this.nombre,\n          correo: this.correo,\n          rol: this.rol,\n          contraseña: this.contraseña\n        });\n        console.log(response.data);\n        const userId = response.data.id;\n        const userName = response.data.nombre;\n        this.$router.push({\n          name: 'DashboardCliente',\n          params: {\n            id: userId,\n            nombre: userName\n          }\n        });\n      } catch (error) {\n        console.error(error);\n        this.mensaje = 'Error al registrar: ' + (error.response?.data || error.message);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","nombre","correo","contraseña","rol","mensaje","methods","registrarUsuario","response","post","console","log","userId","id","userName","$router","push","params","error","message"],"sources":["D:\\CAMILA\\UPC\\Ciclo 6\\Diseño de Experimentos\\Trabajo grupal\\Web-App\\petcare_webapp\\src\\components\\RegistroComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-5\">\r\n    <h1 class=\"text-center mb-4\">Registro de Usuario</h1>\r\n    <form @submit.prevent=\"registrarUsuario\" class=\"border p-4 rounded shadow-sm\">\r\n      <div class=\"mb-3\">\r\n        <label for=\"nombre\" class=\"form-label\">Nombre:</label>\r\n        <input \r\n          v-model=\"nombre\" \r\n          id=\"nombre\" \r\n          type=\"text\" \r\n          class=\"form-control\" \r\n          placeholder=\"Ingrese su nombre\" \r\n          required \r\n        />\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <label for=\"correo\" class=\"form-label\">Correo:</label>\r\n        <input \r\n          v-model=\"correo\" \r\n          id=\"correo\" \r\n          type=\"email\" \r\n          class=\"form-control\" \r\n          placeholder=\"Ingrese su correo\" \r\n          required \r\n        />\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <label for=\"contraseña\" class=\"form-label\">Contraseña:</label>\r\n        <input \r\n          v-model=\"contraseña\" \r\n          id=\"contraseña\" \r\n          type=\"password\" \r\n          class=\"form-control\" \r\n          placeholder=\"Ingrese su contraseña\" \r\n          required \r\n        />\r\n      </div>\r\n      <div class=\"rol\">\r\n        <div class=\"btn-group btn-group-toggle\" data-toggle=\"buttons\">\r\n          <label\r\n              class=\"btn\"\r\n              :class=\"{'btn-secondary': true, 'active': rol === 'dueno'}\">\r\n            <input type=\"radio\" name=\"options\" id=\"option1\" value=\"dueno\" v-model=\"rol\" autocomplete=\"off\"> Dueño\r\n          </label>\r\n          <label\r\n              class=\"btn\"\r\n              :class=\"{'btn-secondary': true, 'active': rol === 'veterinario'}\">\r\n            <input type=\"radio\" name=\"options\" id=\"option2\" value=\"veterinario\" v-model=\"rol\" autocomplete=\"off\"> Veterinario\r\n          </label>\r\n        </div>\r\n      </div>\r\n      <button type=\"submit\" class=\"btn btn-primary w-100 mb-2\">Registrar</button>\r\n      <div v-if=\"mensaje\" class=\"alert alert-danger\" role=\"alert\">\r\n        {{ mensaje }}\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'RegistroComponent',\r\n  data() {\r\n    return {\r\n      nombre: '',\r\n      correo: '',\r\n      contraseña: '',\r\n      rol: 'dueno',\r\n      mensaje: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async registrarUsuario() {\r\n      try {\r\n        const response = await axios.post('http://localhost:8080/api/usuarios/registrar', {\r\n          nombre: this.nombre,\r\n          correo: this.correo,\r\n          rol: this.rol,\r\n          contraseña: this.contraseña,\r\n        });\r\n        console.log(response.data);\r\n        const userId = response.data.id;\r\n        const userName = response.data.nombre;\r\n        this.$router.push({ name: 'DashboardCliente', params: { id: userId, nombre: userName} });\r\n      } catch (error) {\r\n        console.error(error);\r\n        this.mensaje = 'Error al registrar: ' + (error.response?.data || error.message);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Estilos opcionales para mejorar la apariencia */\r\n.container {\r\n  max-width: 400px; /* Limitar el ancho del contenedor */\r\n}\r\n\r\n.border {\r\n  border: 1px solid #e0e0e0; /* Agregar un borde */\r\n}\r\n\r\n.shadow-sm {\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1); /* Sombra ligera */\r\n}\r\n\r\n.alert {\r\n  margin-top: 10px; /* Espacio adicional para la alerta de error */\r\n}\r\n\r\n.rol {\r\n  width: 100%; /* El div ocupará todo el ancho del contenedor */\r\n  margin: 15px 0 15px 0;\r\n}\r\n\r\n.btn-group {\r\n  display: flex; /* Flexbox para alinear los botones horizontalmente */\r\n  justify-content: space-between; /* Opcional: distribuye el espacio entre los botones */\r\n}\r\n</style>\r\n"],"mappings":";AA4DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,MAAM,EAAE,EAAE;MACVC,UAAU,EAAE,EAAE;MACdC,GAAG,EAAE,OAAO;MACZC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,gBAAgBA,CAAA,EAAG;MACvB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMV,KAAK,CAACW,IAAI,CAAC,8CAA8C,EAAE;UAChFR,MAAM,EAAE,IAAI,CAACA,MAAM;UACnBC,MAAM,EAAE,IAAI,CAACA,MAAM;UACnBE,GAAG,EAAE,IAAI,CAACA,GAAG;UACbD,UAAU,EAAE,IAAI,CAACA;QACnB,CAAC,CAAC;QACFO,OAAO,CAACC,GAAG,CAACH,QAAQ,CAACR,IAAI,CAAC;QAC1B,MAAMY,MAAK,GAAIJ,QAAQ,CAACR,IAAI,CA